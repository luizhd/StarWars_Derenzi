<?xml version='1.0' encoding='utf-8'?>
<widget id="br.fib.lp5.StarWars" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Consulta StarWars</name>
    <description>
        
    </description>
    <author email="luizhderenzi@hotmail.com" href="">
        Luiz Henrique
    </author>
	<preference name="Orientation" value="portrait"/>
   <preference name="Fullscreen" value="false"/>
   <preference name="Overscroll" value="false"/>
   <preference name="BackgroundColor" value="0xF2F2F2FF"/>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="~6.0.0" />
    <engine name="ios" spec="~4.3.0" />
    <plugin name="cordova-plugin-device" spec="~1.1.3" />
</widget>
